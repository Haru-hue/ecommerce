{
  "bracketSameLine": true,
  "singleQuote": true,
  "printWidth": 120,
  "useTabs": false,
  "trailingComma": "all",
  "arrowParens": "avoid",
  "semi": false,
  "importOrder": [
    "^reset-css",
    "^react*",
    "models/(.*)$",
    "api/(.*)$",
    "providers/(.*)$",
    "components/(.*)$",
    "routes/(.*)$",
    "utils/(.*)$",
    "assets/(.*)$",
    "^[./]"
  ],
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}